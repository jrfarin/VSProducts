<section id="asideFeatures" data-ob-var="var productJS=new ProductJS(Model)">
                    
    <!-- Brand & Description Text-->
    <div id="textProduct" class="mt-1">                  
        <span class="FS-125 font-weight-normal">Venca</span>    
        <h1 class="FS-11 mt-2">Sujetador bandeau tirantes extraíbles con flores</h1>
    </div>
    
    <!-- Prices & Reviews-->
    <div class="d-flex justify-content-between flex-md-column mt-3 mt-tablet-0">                             
        <div class="mainPrices">
            <span class="d-block FS-075 font-weight-light">desde</span>
            <div class="d-flex mt-1">
                <div id="previousPriceText" class="font-weight-light pr-2">
                    <span><s class="FS-125">19,99€</s></span>
                </div>
                <div id="productprice" class="font-weight-bold text-danger">
                    <span class="FS-125" id="integerPricePart">9</span>
                    <span class="FS-125 ml-n1" id="decimalPricePart">99€</span>
                </div>
            </div>                                 
        </div>                                       
        <div id="reviewsAside" class="mt-sm-0 mt-tablet-0">
            <div class="cPointer" data-target-focus="collapseReview" data-track-event="" data-collapsegroup-target="productInfo">
                <div class="mt-3">
                    <i class="fas fa-star vc-color"></i>
                    <i class="fas fa-star vc-color"></i>
                    <i class="fas fa-star vc-color"></i>
                    <i class="fas fa-star vc-color"></i>
                    <i class="fas fa-star vc-color"></i>
                    <span>(1)</span>
                </div>
            </div>
        </div>
    </div>

    <!-- Sequra Info -->
    <div class="sequraItem mt-3 mt-pdp">
        <span class="FS-1">Compra ahora, paga en 7 días con <img src="https://sandbox.sequracdn.com/assets/images/logos/logo.svg" width="50"></span>
    </div>
    <!-- Sequra Promotions -->
    <div class="FS-1 sequra-promotion-widget sequraItem vc-hide"  data-amount="999" data-product="pp3" data-size="m" data-alignment="left" data-loaded="1">
        <span>Iframe Sequra Promotions go here!!!</span>
    </div>  

    <!-- Notifications Shipping -->
    <div id="notificationsShipping">                                
        <div class="mt-2 mt-pdp">
            <span id="availabilityText" class="FS-1 vc-color vc-hide">Entrega en 48 horas si haces el pedido antes de las 14,00 h.</span>
        </div>  
    </div>    

    <!-- Color & Sizes Mobile -->
    <div id="zoneColorSizeMB" class="d-block d-md-none">
        <div class="row my-4">
            <!-- Colors -->
            <section class="col-6">
                <div class="botonera chevronDown">
                    <a id="colorName" class="d-flex align-items-center justify-content-between  border px-2 py-1" type="button" data-toggle="modal" data-target="#modalChooseColor">
                        <img class="img-fluid" src="https://www.venca.es/i/1245846/ori/sujetador-bandeau-tirantes-extraibles-con-flores-rosa-fucsia.jpg" data-id="1TA140720">                                    
                        <span class="FS-17vh text-uppercase">rosa fucsia</span>
                        <i class="fas fa-chevron-down"></i>
                    </a>                                          
                    <div class="modal fade" id="modalChooseColor" tabindex="-1" role="dialog" aria-labelledby="modalChooseColorLabel" aria-hidden="true">
                        <div class="modal-dialog m-0" role="document">
                            <div class="modal-content vc-vw-height-100">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel">Selecciona color</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">×</span>
                                    </button>
                                </div>                                      
                                <div class="modal-body">
                                    <div data-ob-model="productJS">
                                        <div class="d-flex align-items-center border-bottom pb-2 my-2" data-id="1TA140720" data-dismiss="modal">
                                            <button class="btn-choose-color selected" type="button">
                                                <img src="https://www.venca.es/i/1245846/ori/sujetador-bandeau-tirantes-extraibles-con-flores-rosa-fucsia.jpg" alt="rosa fucsia">
                                            </button>
                                            <div class="text-capitalize pl-2">
                                                <strong>rosa fucsia</strong>
                                            </div>
                                        </div>
                                        <div class="d-flex align-items-center border-bottom pb-2 my-2" data-id="1TA147950" data-dismiss="modal">
                                            <button class="btn-choose-color" type="button">
                                                <img src="https://www.venca.es/i/1245851/ori/sujetador-bandeau-tirantes-extraibles-con-flores-negro.jpg" alt="negro">
                                            </button>
                                            <div class="text-capitalize pl-2">
                                                <strong>negro</strong>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Sizes -->
            <section class="col-6">
                <div class="botonera chevronDown">
                    <a class="vc-height-45 border border-light px-2 py-1 disabledSize d-none" href="JavaScript:;" id="selectedSizeDisabled">TALLA</a>
                    <a id="selectedSize" class="d-flex align-items-center justify-content-between vc-height-45 border px-2 py-1" type="button" data-toggle="modal" data-target="#modalChooseSize">
                        <span class="FS-17vh text-uppercase">Talla</span>
                        <i class="fas fa-chevron-down"></i>
                    </a>                   
                    <!-- Modal Sizes -->                                           
                    <div class="modal fade" id="modalChooseSize" tabindex="-1" role="dialog" aria-labelledby="modalChooseSizeLabel" aria-hidden="true">
                        <div class="modal-dialog m-0" role="document">
                            <div class="modal-content vc-vw-height-100">
                                <div class="modal-header">
                                    <div class="d-flex justify-content-between align-items-center my-3">
                                        <h5 class="modal-title" id="exampleModalLabel">Selecciona Talla</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true" class="d-block FS-15">×</span>
                                        </button>
                                    </div>                                   
                                </div>                                       
                                <div class="modal-body">                                                                                                      
                                    <div id="sizes" data-ob-directive="Product.Mobile.SizeDirective" data-ob-model="productJS">
                                        <ul class="list-unstyled">
                                            <li>
                                                <a href="#" tabindex="1" data-id="36 " data-id-for-select="36" class="">36</a>
                                                <span class="available">Sólo queda(n) 2</span>
                                            </li>
                                            <li class="disabled">
                                                <span data-id="38 " data-id-for-select="38" class="true">38</span>
                                                <span class="available">Agotado</span>
                                            </li>                              
                                            <li>
                                                <a href="#" tabindex="1" data-id="40 " data-id-for-select="40" class="">40</a>
                                            </li>                                     
                                            <li>
                                                <a href="#" tabindex="1" data-id="42 " data-id-for-select="42" class="">42</a>
                                            </li>
                                            <li class="disabled">
                                                <span data-id="44 " data-id-for-select="44" class="true">44</span>
                                                <span class="available">Agotado</span>
                                            </li>
                                            <li class="disabled">
                                                <span data-id="46 " data-id-for-select="46" class="true">46</span>
                                                <span class="available">Agotado</span>
                                            </li>             
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div> 
                </div>

                <!-- Link To Size Guide --> 
                <div class="text-center pt-2" id="SizeGuide" data-vc-id="sizeGuide" data-brand="@Model.TradeBrand">
                    <div class="text-uppercase" data-toggle="modal" data-target="#modalSizeGuide">
                        <span><i class="fas fa-ruler"></i></span>
                        <span class="FS-075 text-muted">Guía de tallas</span>
                        <span><i class="FS-075 fas fa-chevron-right"></i></span>
                    </div>
                </div>
                <!-- Modal SG Mobile -->
                <div class="modal fade" id="modalSizeGuide" tabindex="-1" role="dialog" aria-labelledby="modalSizeGuideLabel"
                    aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title text-uppercase" id="modalSizeGuideLabel">Guía de tallas</h5>
                                <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                    <span class="d-block FS-15" aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                {{> _SizeGuideData }}
                            </div>
                        </div>
                    </div>
                </div>
            </section>                   
        </div>
    </div>

    <!-- Color & Sizes Desktop  -->
    <div id="zoneColorSizeDK" class="d-none d-md-block">
        
        <!-- Colors-->
        <div id="colors" class="row mt-4 mt-pdp">
            <section class="col-md-12">
                <span class="text-uppercase">Color:</span><strong id="colorName" class="FS-1 text-capitalize pl-2">rosa fucsia</strong>
            </section>                                          
            <section class="col-md-12 d-flex flex-wrap mt-3" data-ob-directive="Product.ColorDirective" data-ob-model="productJS">
                <button class="btn-choose-color selected" type="button">
                    <img class="img" src="https://www.venca.es/i/1245846/ori/sujetador-bandeau-tirantes-extraibles-con-flores-rosa-fucsia.jpg" alt="rosa fucsia" data-id="1TA140720">
                </button>
                <button class="btn-choose-color ml-2" type="button">
                    <img class="img" src="https://www.venca.es/i/1245851/ori/sujetador-bandeau-tirantes-extraibles-con-flores-negro.jpg" alt="negro" data-id="1TA147950">
                </button>
            </section>
        </div>
        
        <!-- Sizes -->
        <div id="sizes" class="row my-4 my-tablet-1" data-ob-model="productJS">
            <section class="col-sm-12 col-md-6">
                <div class="dropdown">
                    <button id="dropdownSize" class="w-100 dropdown-toggle bg-white border border-light px-5 py-2" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span id="selectedSize" class="notranslate font-weight-light">TALLA</span>
                    </button>
                    <ul class="FS-17vh dropdown-menu">
                        <li>
                            <a href="#" tabindex="1" data-id="36 " class="notranslate" data-id-for-select="36">36  - sólo queda(n) 2</a>
                        </li>
                        <li class="disabled">
                            <span class="paddingSoldOut" data-id="38 " data-id-for-select="38">38 - Agotado</span>
                        </li>
                        <li>
                            <a href="#" tabindex="1" data-id="40 " class="notranslate" data-id-for-select="40">40  </a>
                        </li>
                        <li>
                            <a href="#" tabindex="1" data-id="42 " class="notranslate" data-id-for-select="42">42 </a>
                        </li>
                        <li class="disabled">
                            <span class="paddingSoldOut" data-id="44 " data-id-for-select="44">44 - Agotado</span>
                        </li>
                        <li class="disabled">
                            <span class="paddingSoldOut" data-id="46 " data-id-for-select="46">46 - Agotado</span>
                        </li>
                    </ul>
                </div>                        
            </section>

            <!-- Sizes Guide -->
            <section class="col-sm-12 col-md-6 px-tablet-0 d-none d-md-block" id="SizeGuide" data-vc-id="sizeGuide" data-brand="@Model.TradeBrand">
                <span class="d-block cPointer py-2" data-toggle="modal" data-target="#modalSizeGuide">
                    <i class="fas fa-ruler"></i>
                    <span class="FS-075 text-uppercase px-2">Guía de tallas</span>
                    <i class="FS-075 fas fa-chevron-right"></i>
                </span>
            </section>
            <!-- Modal SG Desktop -->
            <div class="modal fade" id="modalSizeGuide" tabindex="-1" role="dialog" aria-labelledby="SizeGuideLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title text-uppercase" id="modalSizeGuideLabel">Guía de tallas</h5>
                            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                <span class="d-block FS-15" aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            {{> _SizeGuideData }}
                        </div>
                    </div>
                </div>
            </div>  
        </div>
    </div>                     
    
    <!-- Button ADD & Wishlist DK-->
    <div class="row d-flex align-items-center MX-2 my-phone-0 my-tablet-1 add-button-mb">
        <div id="addProduct" class="col-12 col-md-9">
            <form action="/Cart/AddProductByAjax" data-ob-model="productJS" data-ob-params="{&quot;timeToHide&quot;:5000}" method="post">
                <input id="nodeId" name="nodeId" type="hidden" value="5">
                <input data-val="true" data-val-required="" id="IsChicSizeSpace" name="IsChicSizeSpace" type="hidden" value="False">        
                <button id="addToBasket" class="btn btn-dark vc-button shadow-none addCart" type="button">
                    <span class="FS-125 add">Añadir</span>
                    <span class="added">
                        <i class="fa fa-check fa-lg pr-2"></i><span class="FS-1">Añadido</span>
                    </span>
                    <span class="soldOut vc-hide">Agotado</span>
                </button>
            </form>
        </div>
        <div id="addToWishListDK" class="wishIconAnimate col-12 col-md-3 d-none d-md-block" data-ob-model="productJS" data-ob-params="{&quot;timeToHide&quot;:5000}">
            <button class="border-0 rounded-circle bg-white p-2 pulse-animation"><i class="far fa-heart fa-2x"></i></button>
        </div>
    </div>

    <!-- Seller -->
    <div class="row" id="productSellerDetails">
        <div id="sellerReviews" class="col-12">
            <div class="dflex align-items-center">
                <i class="fas fa-store FS-125"></i>
                <span class=" pl-2">Vendido por</span>
                <span class="text-uppercase text-underlined"><u>Venca</u></span>
                <span class="cPointer vc-color pl-3" data-toggle="modal" data-target="#whatIsThis"><u>¿Qué es esto?</u></span>
                <!-- Modal What's this -->
                {{> _WhatIsThisModal }}
            </div>
            <div class="availability mt-2 vc-hide">
                <div class="d-flex align-items-center">
                    <i class="far fa-clock"></i>                                    
                    <span class="pdl05" id="IsNotAvailableImmediately"></span>
                </div>
            </div>                 
        </div>
    </div>

    <!-- Availability Time -->
    <hr>
    <div  class="row" id="notificationsTime">                          
        <div class="col-12 d-flex align-items-center">
            <i class="far fa-clock FS-125"></i>
            <span class="pl-2" id="IsNotAvailableImmediately">Disponible en 2 semanas</span>
        </div>
    </div>

    <!-- Great Price -->
    <hr>
    <div class="row" id="greatPrice">
        <div class="col-12 d-flex align-items-center">
            <i class="fas fa-tag FS-125"></i>
            <span class="pl-2">Este artículo tiene un precio tan ajustado que no admite descuentos adicionales</span>
        </div>
    </div>   
                   
</section>